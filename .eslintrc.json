{
    "extends": "google",
    "env": { "es6": true, "browser": true },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "rules":{
      "linebreak-style": 0,
      "object-curly-spacing": 0,
      "require-jsdoc": 0,
      "camelcase": 0,
      "max-len": [2, {"code": 120, "tabWidth": 2, "ignoreUrls": true}]
    }
}